package assignment;

public class Display_nu {
	
	
	public void Marks(int mark)
	{
		if(mark>=91&&mark<=100)
		{
			System.out.println("grade A");
		}
		else if(mark>=81&&mark<=90)
		{
			System.out.println("AB");
		}
		else if(mark>=71&&mark<=80)
		{
			System.out.println("bB");
		}
		else if(mark>=61&&mark<=70)
		{
			System.out.println("BC");
		}
		else if(mark>=51&&mark<=60)
		{
			System.out.println("CD");
		}
		else if(mark>=41&&mark<=50)
		{
			System.out.println("DD");
		}
		else 
		{
			System.out.println("fail");
		}
		
		
		
	}
	
	
	

	public static void main(String[] args) {
		// Write a program which will ask the user to enter his/her marks (out of 100). Define a method that will display grades according to the marks entered as below:
//		Marks        Grade
//		91-100         AA
//		81-90          AB
//		71-80          BB
//		61-70          BC
//		51-60          CD
//		41-50          DD
//		<=40          Fail
//		________________________________
		
		Display_nu n1=new Display_nu();
		n1.Marks(30);
	}

}
