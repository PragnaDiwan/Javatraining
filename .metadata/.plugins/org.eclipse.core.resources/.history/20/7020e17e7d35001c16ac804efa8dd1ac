package selenium_Session;

public class Crossbrowsertest {

	public static void main(String[] args) {

		
		String Browser="chrome";
		
		switch (Browser) {
		case "Chrome":
		{
			System.out.println("chrome browser");
			break;
		}
			
		case "firefox":
		{
			System.out.println("chrome browser");
			break;
		}
			
		case "safari":
		{
			System.out.println("chrome browser");
			break;
		}
			

		default:
			System.out.println("please pass the correct browser");
		}
		
	}

}
