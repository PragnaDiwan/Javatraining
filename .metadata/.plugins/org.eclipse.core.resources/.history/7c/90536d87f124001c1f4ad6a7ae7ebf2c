
public class Class_object {

	
	int i=10;//class variable or global variable
	public static void main(String[] args) {
		
		int i=20; //global variable 
		System.out.println(i);
		
		// create the object of class // object create with new keyword
		
		Class_object e1=new Class_object();
		
		System.out.println(e1.i);
		
		//class is the template /blue print or category for the object .it have multiple property
		//object is the physical entity anf it should be created with the help of template(class)
		
		//car,employee,user are class
		
		
		
		
	}

}
